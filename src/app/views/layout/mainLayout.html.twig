	<link rel="stylesheet" href="dist/css/styletools.min.css"  />
	<link rel="stylesheet" href="dist/css/responsive_application.min.css"  />
	
	{% for css in stylesheets %}
		{{ css|raw }}
	{% endfor %}
	
	<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css"  />
	
	<title>{% block title %}Styletools{% endblock title %}</title>
</head>

<body>
	<!--[if lte IE 9]>
		<p class="browserupgrade">You are using an <strong>outdated</strong> browser. Please <a href="https://browsehappy.com/">upgrade your browser</a> to improve your experience and security.</p>
	<![endif]-->
	
	<header>
		<nav class="horizontal_navbar bc_darkblue">
			{{ navicon|raw }}
		</nav>
		
		<div class="dropdown_menu_left bc_darkblue">
			<ul class="navbar_gateways">
				<li><a href="/Styletools-Example/" class="bc_transition">Main page</a></li>
				<li><a href="/Styletools-Example/register" class="bc_transition">Register</a></li>
				<li><a href="/Styletools-Example/login" class="bc_transition">Login</a></li>
				<li><a href="#" {{ modalLink|raw }} class="bc_transition">About</a></li>
			</ul>
		</div>
		
		{{ modal|raw }}
	</header>
	
	{% block body %}
	{% endblock body %}
	
	<footer class="footer bottom_fixed height_10 white bc_darkblue">
		<p class="margin_auto">&copy; 2019 - Styletools</p>
	</footer>
	
	<script src="https://code.jquery.com/jquery-3.3.1.min.js" integrity="sha256-FgpCb/KJQlLNfOu91ta32o/NMZxltwRo8QtmkMRdAu8="></script>
	<script src="/Styletools/dist/js/vendor/modernizr-3.6.0.min.js"></script>
	<script src="//ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
	<script>window.jQuery || document.write('<script src="/Styletools/dist/js/vendor/jquery-3.3.1.min.js"><\/script>')</script>
	<script src="dist/js/styletools.min.js"></script>
	
	{% for js in javascripts %}
		{{ js|raw }}
	{% endfor %}

	<!-- Google Analytics: change UA-XXXXX-Y to be your site's ID. -->
	<script>
		window.ga = function () { ga.q.push(arguments) }; ga.q = []; ga.l = +new Date;
		ga('create', 'UA-XXXXX-Y', 'auto'); ga('send', 'pageview')
	</script>
	<script src="https://www.google-analytics.com/analytics.js" async defer></script>
</body>

</html>